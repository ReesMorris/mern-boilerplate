{
  "name": "mern-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for MERN projects",
  "main": "server.js",
  "scripts": {
    "quick-install": "npm install && cd client && npm install && cd ..",
    "start": "node server",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Rees Morris",
  "license": "MIT",
  "dependencies": {
    "config": "^3.1.0",
    "express": "^4.17.1",
    "mongoose": "^5.5.12",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
  }
}
